package com.liangx.practice.booksystem.entity;

import java.util.Date;

public class LibraryCard {
    private String cardId;

    private Integer borrowedBookNumber;

    private Date cardCreatedDate;

    private Date cardExpirationDate;

    private String cardLogoutFlg;

    public LibraryCard(String cardId, Integer borrowedBookNumber, Date cardCreatedDate, Date cardExpirationDate, String cardLogoutFlg) {
        this.cardId = cardId;
        this.borrowedBookNumber = borrowedBookNumber;
        this.cardCreatedDate = cardCreatedDate;
        this.cardExpirationDate = cardExpirationDate;
        this.cardLogoutFlg = cardLogoutFlg;
    }

    public LibraryCard() {
        super();
    }

    public String getCardId() {
        return cardId;
    }

    public void setCardId(String cardId) {
        this.cardId = cardId == null ? null : cardId.trim();
    }

    public Integer getBorrowedBookNumber() {
        return borrowedBookNumber;
    }

    public void setBorrowedBookNumber(Integer borrowedBookNumber) {
        this.borrowedBookNumber = borrowedBookNumber;
    }

    public Date getCardCreatedDate() {
        return cardCreatedDate;
    }

    public void setCardCreatedDate(Date cardCreatedDate) {
        this.cardCreatedDate = cardCreatedDate;
    }

    public Date getCardExpirationDate() {
        return cardExpirationDate;
    }

    public void setCardExpirationDate(Date cardExpirationDate) {
        this.cardExpirationDate = cardExpirationDate;
    }

    public String getCardLogoutFlg() {
        return cardLogoutFlg;
    }

    public void setCardLogoutFlg(String cardLogoutFlg) {
        this.cardLogoutFlg = cardLogoutFlg == null ? null : cardLogoutFlg.trim();
    }
}